! Xfont settings
Xft.dpi: 220
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb

! xterm
XTerm.vt100.geometry: 80x32
XTerm.vt100.faceName: xos4 Terminus:style=Regular: size=12
XTerm.vt100.selectToClipboard: true

! urxvt
urxvt*font: xft:xos4 Terminus:size=10
urxvt*saveLines:0

! special
*.foreground:   #b9b9b9
*.background:   #101010
*.cursorColor:  #b9b9b9

! black
*.color0:       #101010
*.color8:       #525252

! red
*.color1:       #7c7c7c
*.color9:       #7c7c7c

! green
*.color2:       #8e8e8e
*.color10:      #8e8e8e

! yellow
*.color3:       #a0a0a0
*.color11:      #a0a0a0

! blue
*.color4:       #686868
*.color12:      #686868

! magenta
*.color5:       #747474
*.color13:      #747474

! cyan
*.color6:       #868686
*.color14:      #868686

! white
*.color7:       #b9b9b9
*.color15:      #f7f7f7

! xscreensaver no new user button
xscreensaver.newLoginCommand: 